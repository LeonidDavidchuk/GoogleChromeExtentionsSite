{{#if extensions.length}}
{{#each extensions}}
<div>
    <span>{{Name}}</span>
    <span>{{Price}}</span>
    <span>{{Image}}</span>
    <span>{{Rate}}</span>
</div>
{{/each}}
{{else}}
<h3> No Extensions </h3>
{{/if}}